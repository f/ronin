#!/bin/bash

pip install jinja2
npm install \
  bower \
  uglify-js \
  clean-css \
  node-sass \
  browserify \
  6to5-browserify
cd ./asset

# Install Bower Requirements
../node_modules/.bin/bower install \
    underscore \
    jquery

# You can use git clone
# git clone https://github.com/Rahisify/bxslider-4 bower_components/bxslider-4
cd -
